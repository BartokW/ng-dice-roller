{
  "name": "@ggbetawerks/ng-dice-roller",
  "description": "An Angular Dice Rolling Library",
  "version": "0.0.2",
  "author": "Jonathan Warrington <jonw@ggsoftwerks.com>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "copy:readme": "copyfiles ./README.md ./dist/ng-dice-roller",
    "build": "npm run build:lib && npm run build:website",
    "build:lib": "ng build ng-dice-roller --prod && npm run copy:readme",
    "build:website": "ng build ng-dice-roller-example --prod --base-href='https://ggbetawerks.github.io/ng-dice-roller/'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test",
    "test:lib": "ng test ng-dice-roller",
    "test:lib-coverage": "ng test ng-dice-roller --code-coverage --watch=false",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ng-dice-roller-example --name='Automated Deployment' --email='jonw@ggbetawerks.com'",
    "publish:lib": "cd dist/ng-dice-roller && npm publish",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/ng-dice-roller/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "rxjs": "~6.6.0",
    "three": "^0.121.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/three": "^0.103.2",
    "@types/webgl2": "^0.0.5",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "replace-json-property": "^1.6.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggbetawerks/ng-dice-roller.git"
  },
  "bugs": {
    "url": "https://github.com/ggbetawerks/ng-dice-roller/issues"
  },
  "homepage": "https://ggbetawerks.github.io/ng-dice-roller/",
  "main": "index.js",
  "license": "Apache-2.0"
}
